#include "main.h"

/**
 * print_square - prints diagonal line n times
 *
 * @n: times straight line is printed
 *
 * Return: void
 */

void print_square(int size)
{
	int x, y;

	for (x = 0; x < size; x++)
	{
		for (y = 0; y < size; y++)
		{
		_putchar(35);
		}
		_putchar(92);
		if (x != size - 1)
			_putchar('\n');
	}
	_putchar('\n');
}
