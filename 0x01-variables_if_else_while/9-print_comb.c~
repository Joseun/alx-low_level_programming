#include <stdlib.h>
#include <time.h>
#include <stdio.h>

/**
 * main - Entry point
 *
 * Description - print numbers from 0 to 9
 *
 * Return: Always 0 (Success)
 */

int main(void)
{
	int ch = 0;
	int xy = ch++;

	while (ch < 9)
	{
		putchar(ch + xy);
		putchar(',');
		putchar(' ');
		ch++;
	}
	putchar(int (9));
	putchar('\n');
	return (0);
}
